    <connection>NIKKENLAT_TEST</connection>
    <sql>SELECT&#xa;    T1.CardCode,&#xa;    T0.DocNum,   --Este es el numero de orden que nos piden, tiene el mismo nombre de campo, pero es de otra tabla&#xa;    T1.DocNum as U_Orden,&#xa;	T1.CardName,&#xa;	T1.U_Estafeta,  &#xa;    T2.ItemCode,&#xa;    T2.Dscription as Description,&#xa;    T2.Quantity,&#xa;    T3.E_Mail,&#xa;    T1.DocDate as Fecha_documento&#xa;FROM OINV T1&#xa;INNER JOIN INV1 T2 ON T1.DocEntry &#x3d; T2.DocEntry&#xa;INNER JOIN ORDR T0 ON T0.DocEntry &#x3d; T2.&#x5b;BaseEntry&#x5d; and T2.&#x5b;BaseType&#x5d;&#x3d;17&#xa;LEFT JOIN OCRD T3 ON T1.CardCode &#x3d; T3.CardCode&#xa;-- WHERE  T2.TreeType IN &#x28;&#x27;I&#x27;, &#x27;N&#x27;&#x29; AND T1.DocDate BETWEEN &#x27;2018&#x2f;08&#x2f;21&#x27; AND &#x27;2018&#x2f;08&#x2f;26&#x27;&#xa;-- WHERE  T2.TreeType IN &#x28;&#x27;I&#x27;, &#x27;N&#x27;&#x29; AND T1.DocDate BETWEEN  TRY_CONVERT&#x28;datetime, &#x27;&#x24;&#x7b;BEGIN_DATE&#x7d;&#x27;,111&#x29;&#xa;  --             and      TRY_CONVERT&#x28;datetime, &#x27;&#x24;&#x7b;ENDING_DATE&#x7d;&#x27;,111&#x29;  -- yyyy&#x2f;MM&#x2f;dd&#xa;Where  T2.TreeType IN &#x28;&#x27;I&#x27;, &#x27;N&#x27;&#x29; &#xa;-- AND T1.DocDate &#x3d; &#x27;2018&#x2f;10&#x2f;16&#x27;&#xa;-- AND T1.DocDate &#x3d; &#x27;&#x24;&#x7b;BEGIN_DATE&#x7d;&#x27;&#xa;AND T1.DocDate BETWEEN  TRY_CONVERT&#x28;datetime, &#x27;&#x24;&#x7b;BEGIN_DATE&#x7d;&#x27;,111&#x29; AND TRY_CONVERT&#x28;datetime, &#x27;&#x24;&#x7b;ENDING_DATE&#x7d;&#x27;,111&#x29;&#xa;AND T1.U_Estafeta &#x21;&#x3d; &#x27;Entrega Presencial&#x27;&#x3b;</sql>
    <limit>0</limit>
